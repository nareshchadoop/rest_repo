{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tempdfactory"
		},
		"adls2LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls2LS'"
		},
		"restLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'restLinkedService'"
		},
		"adls2LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tempdatalakegen2.dfs.core.windows.net"
		},
		"restLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://dummy.restapiexample.com/api/v1/employees"
		},
		"restLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "825aa9ad-ac57-4248-a6b6-086a8dd3f83d"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tempdfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls2LS_accountKey": {
						"value": "[parameters('adls2LS_accountKey')]"
					},
					"restLinkedService_password": {
						"value": "[parameters('restLinkedService_password')]"
					},
					"adls2LS_properties_typeProperties_url": {
						"value": "[parameters('adls2LS_properties_typeProperties_url')]"
					},
					"restLinkedService_properties_typeProperties_url": {
						"value": "[parameters('restLinkedService_properties_typeProperties_url')]"
					},
					"restLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('restLinkedService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}